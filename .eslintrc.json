{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "new-cap": "off",
        "indent": ["error", 4],
        "max-len": ["error", 120],
        "object-curly-spacing": ["error", "always"],
        "linebreak-style": ["error", "windows"]
    }
}
